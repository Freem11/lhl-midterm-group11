
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/profile.css" type="text/css" />
    <title>Home Page</title>
    <%- include('partials/_head') %>
  </head>

  <body>
    <%- include('partials/_header') %>
    <div class="user-bit"><%= userInfo[0]['name'] %></div>
    <div class="table-box">
    <table class="favs-table">
      <thead class="tab-head">
        <tr>
          <th scope="col">Favorite Maps</th>
        </tr>
      </thead>
      <tbody>
        <% for(let i = 0; i<favs.length; i++) { %>
          <tr>
            <td class ="tabrow"><a href="/maps/<%= favs[i]['id'] %>"><%= favs[i]['name'] %></a></td>
          </tr>
        <% } %>
      </tbody>
    </table>



    <table class="contribs-table">
      <thead class="tab-head">
        <tr>
          <th scope="col">Map Contributions</th>
        </tr>
      </thead>
      <tbody>
        <% for(let i = 0; i<contribs.length; i++) { %>
          <tr>
            <td class ="tabrow"><a href="/maps/<%= contribs[i]['id'] %>"><%= contribs[i]['name'] %></a></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>


  </body>
</html>
